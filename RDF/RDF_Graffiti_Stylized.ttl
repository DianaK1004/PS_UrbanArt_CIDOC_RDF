        aat:300444972 ;  # Publicly Accessible
    crm:P102_has_title [
        a crm:E35_Title ;
        crm:P190_has_symbolic_content "Stylized Graffiti"@en ;
        crm:P2_has_type aat:300417199  # Descriptive Title
    ] ;
    # Dimensions (approximated for the test)
    crm:P43_has_dimension [
        a crm:E54_Dimension ;
        crm:P2_has_type aat:300055647 ;            
        crm:P90_has_value "2.0"^^xsd:decimal ;
        crm:P91_has_unit aat:300379099             
    ] ;
    crm:P43_has_dimension [
        a crm:E54_Dimension ;
        crm:P2_has_type aat:300055644 ;            
        crm:P90_has_value "5.0"^^xsd:decimal ;
        crm:P91_has_unit aat:300379099
    ] ;
    crm:P129_is_about 
        ex:PublicExpression , 
        ex:SocialProtest , 
        ex:EmotionVulnerability ;

    crm:P65_shows_visual_item ex:MuralVisual_UnknownGraffiti ;
    crm:P108i_was_produced_by ex:Production_UnknownGraffiti ;
    crm:P44_has_condition ex:ConditionState_Good ;
    crm:P34i_was_assessed_by ex:ConditionAssessment_UnknownGraffiti .

# Visual elements 
ex:MuralVisual_UnknownGraffiti a crm:E36_Visual_Item ;
    crm:P2_has_type aat:300248231 ; # Visual Description
    crm:P138_represents 
        ex:Clouds , 
        ex:Person , 
        ex:Heart ;
    crm:P45_consists_of ex:PinkColor , ex:YellowColor , ex:BlueColor , ex:WhiteColor ;
    crm:P137_exemplifies aat:300256621 ; # Political Art
    crm:P165_incorporates ex:Tag_Yaratun , ex:Slogan_NoGods , ex:ArabicText_Breathe .

# Production event
ex:Production_UnknownGraffiti a crm:E12_Production ;
    crm:P2_has_type aat:300054713 ; # Production
    crm:P14_carried_out_by ex:UnknownArtist ;
    crm:P4_has_time-span [
        a crm:E52_Time-Span ;
        crm:P82a_begin_of_the_begin "2025-01-01"^^xsd:date ;
        crm:P82b_end_of_the_end "2025-12-31"^^xsd:date
    ] ;
    crm:P7_took_place_at ex:Graz_LendDistrict ;
    crm:P126_employed aat:300015058 ; # Acrylic paints
    crm:P32_used_general_technique [
        a crm:E55_Type ;
        crm:P2_has_type aat:300182574 ;   # Acrylic painting
        crm:P1_is_identified_by [
            a crm:E41_Linguistic_Appellation ;
            crm:P190_has_symbolic_content "Looks like a layered technique using acrylic paint and markers"@en
        ]
    ] .

# Condition
ex:ConditionState_Good a crm:E3_Condition_State ;
    crm:P2_has_type ex:GoodCondition ;
    crm:P3_has_note "Artwork's colors are fading, some damage to the wall is visible, but overall condition is good"@en .

# Condition detailed
ex:ConditionAssessment_UnknownGraffiti a crm:E14_Condition_Assessment ;
    crm:P4_has_time-span [
        a crm:E52_Time-Span ;
        crm:P82a_begin_of_the_begin "2025-04-01"^^xsd:date
    ] ;
    crm:P14_carried_out_by ex:AnonymousObserver ;
    crm:P34_assessed_condition_of ex:ConditionState_Good .

# Observations (CRMsci)
ex:Observation_April2025 a crmsci:S4_Observation ;
    crm:P14_carried_out_by ex:AnonymousObserver ;
    crm:P4_has_time-span [
        a crm:E52_Time-Span ;
        crm:P82a_begin_of_the_begin "2025-04-16"^^xsd:date ;
        crm:P82b_end_of_the_end "2025-04-16"^^xsd:date
    ] ;
    crmsci:O8_observed ex:Unknown_Graffiti ;
    crm:P3_has_note "Graffiti is still visible in April 2025."@en .

ex:Observation_June2025 a crmsci:S4_Observation ;
    crm:P14_carried_out_by ex:AnonymousObserver ;
    crm:P4_has_time-span [
        a crm:E52_Time-Span ;
        crm:P82a_begin_of_the_begin "2025-06-10"^^xsd:date ;
        crm:P82b_end_of_the_end "2025-06-10"^^xsd:date
    ] ;
    crmsci:O8_observed ex:Unknown_Graffiti ;
    crm:P3_has_note "Graffiti no longer visible, mural overpainted."@en .

# Destruction event
ex:Destruction_UnknownGraffiti a crm:E6_Destruction ;
    crm:P13_destroyed ex:Unknown_Graffiti ;
    crm:P4_has_time-span [
        a crm:E52_Time-Span ;
        crm:P82a_begin_of_the_begin "2025-04-16"^^xsd:date ;
        crm:P82b_end_of_the_end "2025-06-30"^^xsd:date
    ] ;
    crm:P7_took_place_at ex:Graz_LendDistrict ;
    crm:P14_carried_out_by ex:OverpaintingArtist .

# Overpainting viewed as one activity that consists of Destruction + Production
ex:Overpainting_Activity a crm:E7_Activity ;
    crm:P14_carried_out_by ex:OverpaintingArtist ;
    crm:P7_took_place_at ex:Graz_LendDistrict ;
    crm:P4_has_time-span [
        a crm:E52_Time-Span ;
        crm:P82a_begin_of_the_begin "2025-04-16"^^xsd:date ;
        crm:P82b_end_of_the_end "2025-06-30"^^xsd:date
    ] ;
    crm:P9_consists_of ex:Destruction_UnknownGraffiti , ex:Production_OverpaintingGraffiti .

# Production event for the new overpainting
ex:Production_OverpaintingGraffiti a crm:E12_Production ;
    crm:P108_has_produced ex:Overpainting_Graffiti ;
    crm:P14_carried_out_by ex:OverpaintingArtist ;
    crm:P7_took_place_at ex:Graz_LendDistrict ;
    crm:P4_has_time-span [
        a crm:E52_Time-Span ;
        crm:P82a_begin_of_the_begin "2025-04-16"^^xsd:date ;
        crm:P82b_end_of_the_end "2025-06-30"^^xsd:date
    ] .

