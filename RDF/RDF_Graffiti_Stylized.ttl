    crm:P1_is_identified_by [ a crm:E41_Appellation ; rdf:value "David Leiner" ] .

# Inscriptions 
ex:Tag_Yaratun a crm:E34_Inscription ;
    crm:P2_has_type aat:300255231 ;   # Lettering
    crm:P3_has_note [ a crm:E62_String ;
        crm:P190_has_symbolic_content "YARATUN"
    ] ;
    crm:P129_is_about ex:Yaratun_Concept .

ex:Slogan_NoGods a crm:E34_Inscription ;
    crm:P2_has_type aat:300255231 ; # Lettering
    crm:P3_has_note [ a crm:E62_String ;
        crm:P190_has_symbolic_content "NO GODS NO MASTERS"
    ] ;
    crm:P129_is_about ex:NoGodsNoMasters_Concept .

ex:ArabicText_Breathe a crm:E34_Inscription ;
    crm:P2_has_type aat:300400697 ; # Arabic script (type)
    crm:P3_has_note [ a crm:E62_String ;
        crm:P190_has_symbolic_content "تنفسوا"
    ] ;
    crm:P129_is_about ex:Breathe_Concept .

# Concepts referenced by the inscriptions 
ex:Yaratun_Concept a crm:E28_Conceptual_Object ;
    rdfs:label "Graffiti tag/signature: 'YARATUN'"@en ;
    skos:note "Interpreted as a marker of authorship or identity (individual or crew), associating the work with a name/alias."@en .

ex:NoGodsNoMasters_Concept a crm:E28_Conceptual_Object ;
    rdfs:label "Anti‑authoritarian motto: 'No gods, no masters'"@en ;
    skos:note "A slogan associated with anarchist and labor movements; signals resistance to hierarchical control and an assertion of autonomy."@en .

ex:Breathe_Concept a crm:E28_Conceptual_Object ;
    rdfs:label "Call to 'Breathe!'"@en ;
    skos:note "A prompt to pause and endure; in protest or stress contexts, can signal resistance to forms of suffocation—literal, social, or psychological."@en .

